module seg_led_static_top(
	//输入信号定义
	input sys_clk,//系统时钟信号
	input sys_rst_n,//系统复位信号
	
	//输出信号定义
	output [5:0] sel,//数码管显示控制（位选信号）
	output [7:0] sel_led,//数码管显示数据（段选信号）
	
);

//参数数据类型的数据定义
