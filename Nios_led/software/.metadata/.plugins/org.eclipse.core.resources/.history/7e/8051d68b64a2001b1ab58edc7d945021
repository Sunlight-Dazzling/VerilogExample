
#include <stdio.h>
#include "system.h"
#include "altera_avalon_pio_regs.h"
int main()
{
  //printf("Hello world\n");
  while(1){
	  IOWR_ALTERA_AVALON_PIO_DATA(LED_BASE,1);
	  IOWR_ALTERA_AVALON_PIO_DATA(LED_BASE,0);
	  IOWR_ALTERA_AVALON_PIO_DATA(LED_BASE+1,1);
	  IOWR_ALTERA_AVALON_PIO_DATA(LED_BASE,0);
	  IOWR_ALTERA_AVALON_PIO_DATA(LED_BASE+2,1);
	  IOWR_ALTERA_AVALON_PIO_DATA(LED_BASE,0);
	  IOWR_ALTERA_AVALON_PIO_DATA(LED_BASE+3,1);
	  IOWR_ALTERA_AVALON_PIO_DATA(LED_BASE,0);
  }
  return 0;
}
